package com.CPT.Admin.Servlet;

import java.io.IOException;
import java.nio.file.Path;

import javax.servlet.ServletException;
import javax.servlet.http.HttpServlet;
import javax.servlet.http.HttpServletRequest;
import javax.servlet.http.HttpServletResponse;
import javax.servlet.http.Part;

public class AddSanPhams extends HttpServlet{

	private static final long serialVersionUID = 1L;

	@Override
	protected void doPost(HttpServletRequest req, HttpServletResponse resp) throws ServletException, IOException {
		int idDanhMuc = Integer.parseInt(req.getParameter(""));
		String Name = req.getParameter("");
		String GiaCu	= req.getParameter("");
		String GiaMoi	=	req.getParameter("");
		String MoTa 	= req.getParameter("");
		Part Image	= req.getPart("");
		String FileImage = Path.get();
	}

}
